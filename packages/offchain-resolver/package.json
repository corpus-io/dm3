    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@prisma/client": "^4.15.0",
        "body-parser": "^1.20.1",
        "dm3-lib-crypto": "workspace:^",
        "dm3-lib-profile": "workspace:^",
        "dm3-lib-shared": "workspace:^",
        "dotenv": "^16.0.3",
        "eslint": "^8.17.0",
        "ethers": "5.7.2",
        "express": "^4.18.1",
        "jayson": "^3.6.6",
